       <string notr="true">QTabWidget::pane { /* The tab widget frame */
    border-top: 2px solid #3a5269;
}

QTabWidget::tab-bar {
    left: 2px; /* move to the right by 5px */
}

/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #3a5269, stop:1 #2c3e50);
    border: 2px solid #212f3c;
	color:white;
    border-bottom-color: #33485d; /* same as the pane color */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 21ex;
    padding: 2px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4a6986, stop:1 #415c76);
}

QTabBar::tab:selected {
    border-color: #2f4355;
	color:rgb(38, 183, 255);
    border-bottom-color: #3a5269; /* same as pane color */
}

QTabBar::tab:!selected {
    margin-top: 2px; /* make non-selected tabs look smaller */
}</string>
